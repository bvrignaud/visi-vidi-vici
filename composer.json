{
    "name": "bvrignaud/visi-vidi-vici",
    "type": "project",
    "description": "Clear sea weather for freedivers and underwater fishermen.",
    "keywords": ["apnea", "sea", "spearfishing", "turbidity", "visibility"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^v0.6.3",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "sentry/sentry-laravel": "^4.2.0",
        "silviolleite/laravelpwa": "^2.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^v7.3.1",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "laravel-lang/common": "^6.1",
        "laravel/pint": "^1.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/phpinsights": "^2.7",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan lang:update",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pintd": [
            "@php ./vendor/bin/pint --dirty"
        ],
        "stan": [
            "@php ./vendor/bin/phpstan analyse"
        ],
        "clean": [
            "@php ./vendor/bin/pint --dirty",
            "@php ./vendor/bin/phpstan analyse",
            "@php artisan test --parallel"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
